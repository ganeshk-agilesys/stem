<div id="message"></div>

<div class="span-45 last half_padding">
  <div class="span-44 last half_padding box2 box_grey">
    <h2><%= t(:assess_your_job_skills_for_this_career_path) %></h2>
    <div class="span-3">
      <%= image_tag "jp_asst.png" %>
    </div>
    <div class="span-41 last">
      <span><%= t(:job_skills_tab_text) %></span>
    </div>
  </div>

  <%- if @occupation.has_key?("work_activities")-%>
  <%#- @occupation.work_activities.each do |work_activity| -%>
    <div class="span-45 last prepend-top box2 skill_section">
      <div class="span-44 box_grey half_padding">
        <h2><%= t(:skill_category) %>: <%#= work_activity.element_name %></h2>
        <h2><%= t(:importance) %>: <%#= stars_for(work_activity.importance) %></h2>
      </div>
    </div>
  <%#- end -%>
<%- end -%>
</div>